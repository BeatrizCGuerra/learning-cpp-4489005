#include <iostream>
#include <string>
#include <cstring>

int main(){
    float flt;
    int32_t sgn;
    uint32_t unsgn;

    flt = -7.66;
    sgn = flt;
    unsgn = sgn;

    std::cout << "float = " << flt << std::endl;
    std::cout << "sgn = " << sgn << std::endl;
    std::cout << "unsgn = " << unsgn << std::endl;

    std::cout << std::endl << std::endl;
    return (0);
}